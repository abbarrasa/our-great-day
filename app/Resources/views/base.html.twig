        {% block body %}
        <div class="index-page sidebar-collapse">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg bg-primary fixed-top navbar-transparent " color-on-scroll="100">
                <div class="container">
                    <div class="navbar-translate">
                        <a class="navbar-brand" href="{{ path('homepage') }}">
                            {{ 'frontend.title'|trans }}
                        </a>
                        <button class="navbar-toggler navbar-toggler" type="button" data-toggle="collapse" data-target="#navigation" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-bar bar1"></span>
                            <span class="navbar-toggler-bar bar2"></span>
                            <span class="navbar-toggler-bar bar3"></span>
                        </button>
                    </div>
                    {% image '../app/Resources/public/images/blurred-image-1.jpg' %}
                    <div class="collapse navbar-collapse justify-content-end" id="navigation" data-nav-image="{{ asset_url }}">
                        {{ knp_menu_render('main', {'template': 'partials/knp_menu.html.twig'}) }}
                        {{ knp_menu_render('social', {'template': 'partials/knp_menu.html.twig'}) }}
                    </div>
                    {% endimage %}
                </div>
            </nav>
            <!-- End Navbar -->

            <div class="wrapper">
                <div class="page-header clear-filter" filter-color="purple">
                {% image '../app/Resources/public/images/header.jpg' %}
                    <div class="page-header-image" data-parallax="true" style="background-image: url('{{ asset_url }}');"></div>
                {% endimage %}

                    <div class="container">
                        <div class="content-center brand">
                            {#<img class="n-logo" src="./assets/img/now-logo.png" alt="">#}
                            <h1 class="h1-seo">Vero y Carlos</h1>
                            <h3>{{ 'frontend.title'|trans }}</h3>
                        </div>
                        <a href="" data-scroll="true" data-id="#first-section" class="scroll-arrow hidden-xs hidden-sm">
                            <i class="fa fa-angle-down"></i>
                        </a>

                        {#<h6 class="category category-absolute">Designed by#}
                            {#<a href="http://invisionapp.com/" target="_blank">#}
                                {#<img src="./assets/img/invision-white-slim.png" class="invision-logo" />#}
                            {#</a>. Coded by#}
                            {#<a href="https://www.creative-tim.com" target="_blank">#}
                                {#<img src="./assets/img/creative-tim-white-slim2.png" class="creative-tim-logo" />#}
                            {#</a>.</h6>#}
                    </div>
                </div>

                <!-- Main -->
                <div class="main">
                    {% block main %}{% endblock %}
                </div>
                <!-- End Main -->

                <!-- Modal -->
                <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
