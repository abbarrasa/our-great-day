{% extends '@SonataAdmin/CRUD/base_list_field.html.twig' %}

{% block field %}
    {% set file = attribute(object, field_description.name) %}
    {% if file is not null %}
        <a href="{{ admin.generateUrl('edit', {'id' : object|sonata_urlsafeid }) }}">
            <img src="{{ file.pathname|data_uri }}" width="70" height="45" />
        </a>
    {% endif %}
{% endblock %}
